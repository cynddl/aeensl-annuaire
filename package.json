{
  "name": "annuaire",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "build:js": "browserify -t babelify assets/js/app.js -o assets/dist/app.js",
    "watch:js": "watchify -t babelify assets/js/app.js -o assets/dist/app.js",
    "build:sass": "node-sass assets/sass/main.scss assets/dist/main.css",
    "watch:sass": "node-sass -wr assets/sass/main.scss assets/dist/main.css",
    "build:img": "cp -R node_modules/intl-tel-input/build/img assets/dist/img",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --port 8086",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "author": "Luc Rocher",
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "bootstrap-vue": "^0.7.1",
    "element-ui": "^1.1.6",
    "intl-tel-input": "^9.2.4",
    "places.js": "^1.4.7",
    "select2": "^4.0.3",
    "vee-validate": "^2.0.0-beta.22",
    "vue": "^2.1.10"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-component": "^0.9.0",
    "babel-preset-es2015": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "cross-env": "^3.1.4",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.10.0",
    "node-sass": "^3.11.3",
    "style-loader": "^0.13.1",
    "vue-loader": "^10.3.0",
    "vue-template-compiler": "^2.1.10",
    "watchify": "^3.7.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  }
}
